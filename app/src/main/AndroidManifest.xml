<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.amy.scoringapp" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AdminActivity"
            android:label="@string/title_activity_admin" >
        </activity>
        <activity
            android:name=".AdminLogin"
            android:label="@string/title_activity_admin_login" >
        </activity>
        <activity
            android:name=".LoginError"
            android:label="@string/title_activity_login_error" >
        </activity>
        <activity
            android:name=".NotificationBoard"
            android:label="@string/title_activity_notification_board" >
        </activity>
        <activity
            android:name=".TournamentPassword"
            android:label="@string/title_activity_tournament_password" >
        </activity>
        <activity
            android:name=".EnterScore"
            android:label="@string/title_activity_enter_score" >
        </activity>
        <activity
            android:name=".AdminOptions"
            android:label="@string/title_activity_admin_options" >
        </activity>
        <activity
            android:name=".AdminAddOrDelete"
            android:label="@string/title_activity_admin_add_or_delete" >
        </activity>
    </application>

</manifest>
